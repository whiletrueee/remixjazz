generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model users {
  id    Int    @id @default(autoincrement())
  email String @unique(map: "unique_user_email")
}

model food {
  id    Int    @id @default(autoincrement())
  name String @unique(map: "unique_food_name")
}